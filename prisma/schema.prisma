datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  guild_id String @unique @map("guild_id")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  guild_id String
  user_id  String
}
