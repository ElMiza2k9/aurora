datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  guild_id String @unique
  locale   String @default("en-US")

  @@map("guilds")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  guild_id String
  user_id  String

  @@map("users")
}
